pipeline{

    agent any

    stages {

        stage ('checkout') {

            steps {
                echo 'git checkout done'
            }
        }
        stage ('build') {

            steps {
                echo 'build succed'
            }
        }
        stage ('test') {

            steps {

                echo 'test succed'
            }
        }


        stage ('deployment') {

            steps {

                echo 'deployment succed'
            }
        }


        stage ('delivery') {

            steps {

                echo 'delivery succed'
            }
        }

    }
}
